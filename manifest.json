{
  "manifest_version": 3,
  "name": "Tab Volume Control",
  "version": "1.0",
  "description": "Control the volume of the current tab.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
